{
    "name": "lch/menu-bundle",
    "type": "symfony-bundle",
    "description": "Symfony LchMenuBundle",
    "keywords": ["Menu management"],
    "homepage": "https://github.com/compagnie-hyperactive/MenuBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas BONNIOT",
            "email": "nbonniot@compagnie-hyperactive.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/symfony": "~3.1",
        "knplabs/doctrine-behaviors": "~1.1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3",
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "conflict": {
        "symfony/doctrine-bridge": "<2.7"
    },
    "autoload": {
        "psr-4": {
            "Lch\\MenuBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
